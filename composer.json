{
    "name": "luoluolzb/lqf",
    "description": "luoluolzb's another php framework",
    "keywords": ["lqf", "framework"],
    "homepage": "https://github.com/luoluolzb/lqf",
    "authors": [
        {
            "name": "luoluolzb",
            "email": "luoluolzb@163.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "luoluolzb/di": "^1.0",
        "nikic/fast-route": "^1.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4",
        "phpstan/phpstan": "^0.11.12"
    },
    "autoload": {
        "psr-4": {
            "lqf\\": "src/"
        }
    },
    "scripts": {
        "phpcs": "vendor\\bin\\phpcs -sw --standard=PSR2 --colors src",
        "phpcbf": "vendor\\bin\\phpcbf -sw --standard=PSR2 --colors src",
        "phpstan": "vendor\\bin\\phpstan analyse --level 7 src"
    }
}
